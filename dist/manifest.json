{
  "manifest_version": 3,
  "name": "Paragon Userkeys",
  "version": "0.2.0",
  "content_scripts": [
    {
      "matches": ["https://*.paragonrels.com/*"],
      "all_frames": true,
      "js": ["./content_import.js"]
    }
  ],
  "options_page": "index.html",
  "actions": {
    
  }
  "web_accessible_resources": [
    {
      "resources": ["content/*"],
      "matches": ["https://*.paragonrels.com/*"]
    }
  ],
  "permissions": [
    "storage"
  ]
}